<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/image_kanshu"
        android:layout_width="@dimen/height_12_80"
        android:layout_height="@dimen/height_12_80"
        android:layout_marginTop="@dimen/height_5_80"
        android:layout_gravity="center_horizontal"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_6"
        android:id="@+id/text1"
        android:layout_marginTop="@dimen/height_2_80"
        android:layout_gravity="center_horizontal"
        />

    <ImageView
        android:layout_width="@dimen/height_12_80"
        android:layout_height="@dimen/height_12_80"
        android:id="@+id/image_kehu"
        android:layout_marginTop="@dimen/height_5_80"
        android:layout_gravity="center_horizontal"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_6"
        android:id="@+id/text2"
        android:layout_marginTop="@dimen/height_2_80"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_user"
        android:layout_marginTop="@dimen/height_5_80"
        android:layout_gravity="center_horizontal"
        />

    <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
        android:id="@+id/button_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:paddingBottom="10dp"
        android:paddingLeft="@dimen/height_5_80"
        android:paddingRight="@dimen/height_5_80"
        android:layout_marginTop="@dimen/height_3_80"
        android:paddingTop="10dp"
        android:text="退出登陆"
        android:textSize="@dimen/text_size_6"
        app:qmui_radius="4dp" />
</LinearLayout>
